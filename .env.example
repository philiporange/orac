# Jinn Configuration Environment Variables
# Copy this file to .env and modify the values as needed

# LLM Configuration
ORAC_LLM_PROVIDER=google
ORAC_DEFAULT_MODEL_NAME=gemini-2.0-flash

# Paths
ORAC_DEFAULT_PROMPTS_DIR=./orac/prompts
ORAC_DEFAULT_CONFIG_FILE=./orac/config.yaml
ORAC_DOWNLOAD_DIR=/tmp/orac_downloads
ORAC_LOG_FILE=./llm.log

# API Keys (used by the application)
GOOGLE_API_KEY=your_google_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
